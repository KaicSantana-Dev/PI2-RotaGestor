generator client {
  provider = "prisma-client-js"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id        String  @id @default(uuid())
  nome      String
  sobrenome String?
  idade     Int
}

model Veiculos {
  id     String @id @default(uuid())
  Modelo String
  Marca  String
  Placa  String
}
